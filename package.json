{
  "name": "loot",
  "version": "0.2.0",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "test": "export NODE_ENV=test && mocha --exit --require babel-polyfill --require babel-register",
    "start": "export NODE_ENV=production && npm run clean && npm run front:build && npm run app:start",
    "app:start": "node app",
    "front:build": "mkdir -p static && webpack --mode production --config config/webpack.config.js",
    "dev": "export NODE_ENV=development && npm run clean && (npm run front:dev & npm run app:dev)",
    "app:dev": "nodemon --watch app --watch config",
    "front:dev": "mkdir -p static && webpack --watch --mode development --config config/webpack.config.js",
    "clean": "rm -rf static"
  },
  "repository": {
    "type": "git",
    "url": "ssh://loiic/var/git/loot.git"
  },
