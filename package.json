    "log4js": "^2.6.1",
    "mongoose": "^5.1.1",
    "npm": "^6.1.0",
    "pug": "^2.0.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "update": "^0.7.4"
  }
}
